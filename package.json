{
  "name": "2k1w-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service renderSkin && vue-cli-service serve",
    "build": "vue-cli-service build && node gitInfo.js",
    "lint": "eslint --ext .js,.vue src -f html -o autoTarget/eslint/lint.html",
    "build:theme": "vue-cli-service renderSkin && skb run",
    "cz:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "lego": "lego ui"
  },
  "dependencies": {
    "@hui-pro/empty": "^1.18.0",
    "@hui-pro/layout": "^2.0.0-alpha.6",
    "@hui-pro/locale": "^1.0.0",
    "@hui-pro/page": "^2.0.0-alpha.6",
    "@hui-pro/utils": "^1.0.0",
    "@hui/svg-icon": "^1.0.0",
    "axios": "^0.18.0",
    "core-js": "^3.6.5",
    "current-script-polyfill": "^1.0.0",
    "dolphin-plugin-tools": "^2.3.0",
    "hui": "^2.7.3",
    "traceservice-instrumentation-axios": "^0.3.0",
    "vue": "^2.6.11",
    "vue-i18n": "^7.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "commitizen": "^3.0.5",
    "conventional-changelog": "^3.0.5",
    "conventional-changelog-cli": "^2.0.12",
    "cz-customizable": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^1.1.1",
    "lego": "^1.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mkdirp": "^0.5.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "skin-bundle": "^1.0.1",
    "vue-cli-plugin-changelog": "^2.9.0",
    "vue-cli-plugin-dolphin-base": "^2.9.0",
    "vue-cli-plugin-dolphin-theme": "^2.9.0",
    "vue-cli-plugin-easymock": "^2.9.0",
    "vue-cli-plugin-easytool": "^2.9.0",
    "vue-cli-plugin-lego": "^2.9.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "ignoredNodes": [
            "TemplateLiteral"
          ]
        }
      ],
      "template-curly-spacing": [
        "off"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
